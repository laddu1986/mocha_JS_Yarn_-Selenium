{
  "name": "qa-automation",
  "version": "1.0.0",
  "description": "qa-automation framework for e2e testing using node and webdriverio",
  "main": "wdio.appcurator.js",
  "dependencies": {
    "app-module-path": "^2.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chakram": "^1.5.0",
    "clipboardy": "^1.2.3",
    "config-yml": "^0.8.0",
    "dateformat": "^3.0.3",
    "dotenv": "^6.0.0",
    "mysql2": "^1.5.3",
    "node-es-module-loader": "^0.3.8",
    "npm": "^6.0.0",
    "pngjs": "^3.3.3",
    "randomstring": "^1.1.5",
    "sequelize": "^4.37.10",
    "wdio-html-format-reporter": "0.2.7",
    "wdio-mocha-framework": "^0.5.13",
    "wdio-spec-reporter": "^0.1.4",
    "webdriverio": "^4.12.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-mocha": "^5.2.0",
    "lint-staged": "^7.3.0",
    "prettier": "1.14.3"
  },
  "scripts": {
    "web": "node_modules/.bin/wdio wdio.appcurator.js",
    "web-debug": "DEBUG=true ./node_modules/.bin/wdio wdio.appcurator.js",
    "orca": "mocha test/orca/specs/loginTest.js --require babel-register",
    "clean": "node_modules/.bin/eslint 'test/**/*.js' --fix",
    "lint": "node_modules/.bin/eslint 'test/**/*.js'",
    "precommit": "lint-staged",
    "web-pipelines": "node_modules/.bin/wdio wdio.bitbucket-pipelines.js",
    "web-smoke-tests": "node_modules/.bin/wdio wdio.appcurator.js --suite smoke --mochaOpts.grep=@smoke"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/appcurator/qa-automation"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "QA Team",
  "license": "UNLICENSED",
  "pre-commit": [
    "precommit"
  ]
}
