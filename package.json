{
  "name": "qa-automation",
  "version": "1.0.0",
  "description": "qa-automation framework for e2e testing using node and webdriverio",
  "main": "wdio.appcurator.js",
  "dependencies": {
    "app-module-path": "^2.2.0",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "chakram-joi": "^1.0.0",
    "config-yml": "^0.8.0",
    "copy-paste": "^1.3.0",
    "dateformat": "^3.0.3",
    "dotenv": "^6.0.0",
    "eslint": "^4.18.2",
    "eslint-plugin-babel": "^5.1.0",
    "mocha-multi-reporters": "^1.1.7",
    "mocha-simple-html-reporter": "^1.1.0",
    "mysql2": "^1.5.3",
    "node-es-module-loader": "^0.3.8",
    "npm": "^6.0.0",
    "pngjs": "^3.3.3",
    "randomstring": "^1.1.5",
    "sequelize": "^4.37.10",
    "wdio-dot-reporter": "^5.0.0-alpha.7",
    "wdio-html-format-reporter": "0.2.7",
    "webdriverio": "^4.12.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "allure-commandline": "^2.0.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chakram": "^1.5.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "mocha": "^5.0.5",
    "wdio-mocha-framework": "^0.5.13",
    "wdio-spec-reporter": "^0.1.4"
  },
  "scripts": {
    "web": "node_modules/.bin/wdio wdio.appcurator.js",
    "api": "mocha test/api/specs/*Test.js --opts test/api/config/mocha.opts",
    "report": "allure generate allure-results --clean -o allure-report && allure open allure-report",
    "orca": "mocha test/orca/specs/loginTest.js --require babel-register",
    "test": "./node_modules/mocha/bin/mocha test/api/specs/*Test.js --require babel-register",
    "clean": "node_modules/.bin/eslint test/* --fix",
    "lint": "jshint .",
    "validate": "npm ls",
    "web-pipelines": "node_modules/.bin/wdio wdio.bitbucket-pipelines.js",
    "api-pipelines": "mocha test/api/specs/*Test.js --require babel-register --timeout 10000",
    "web-smoke-tests": "node_modules/.bin/wdio wdio.appcurator.js --mochaOpts.grep=@smoke",
    "api-smoke-tests": "mocha test/api/specs/*Test.js --opts test/api/config/mocha.opts --grep @smoke"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/appcurator/qa-automation"
  },
  "author": "QA Team",
  "license": "UNLICENSED",
  "pre-commit": []
}